<?xml version="1.0" encoding="UTF-8" ?>
<schema xmlns="http://www.w3.org/2001/XMLSchema"
        targetNamespace="http://server.talend.telefonica.de/"
        xmlns:userns="http://server.talend.telefonica.de/"
        elementFormDefault="qualified">

    <!-- todo: check -->
    <element name="ticketInfo" type="userns:TicketInfo"></element>
    <element name="ticketResponseList" type="userns:TicketResponseList"></element>

    <!-- request -->
    <complexType name="TicketInfo">
        <sequence>
            <element name="transactionHeader" type="userns:TransactionHeader"></element>
            <element name="transactionBody" type="userns:TransactionBody"></element>
        </sequence>
    </complexType>
    <complexType name="TransactionHeader">
        <sequence>
            <element name="source" type="string"></element>
            <element name="destination" type="string"></element>
            <!-- todo: update -->
            <element name="method" type="string"></element>
        </sequence>
    </complexType>
    <complexType name="TransactionBody">
        <sequence>
            <element name="supportGroupList" type="userns:SupportGroup"></element>
        </sequence>
    </complexType>
    <complexType name="SupportGroup">
        <sequence>
            <element name="supportGroup" type="string" minOccurs="0" maxOccurs="unbounded"></element>
        </sequence>
    </complexType>

    <!-- response -->
    <complexType name="TicketResponseList">
        <sequence>
            <element name="ticketResponse" type="userns:TicketResponse" minOccurs="0" maxOccurs="unbounded"></element>
        </sequence>
    </complexType>
    <complexType name="TicketResponse">
        <sequence>
            <element name="CHANGE_REQUEST_ID" type="string"></element>
            <element name="COORDINATOR_GROUP" type="string"></element>
            <element name="CHANGE_LOCATION" type="string"></element>
            <element name="SERVICE" type="string"></element>
            <element name="CI_ID" type="string"></element>
            <element name="CI_NAME" type="string"></element>
            <element name="LOCATION_CI" type="string"></element>
            <element name="TEMPLATE" type="string"></element>
            <element name="SUMMARY" type="string"></element>
            <element name="NOTES" type="string"></element>
            <element name="CRQCLASS" type="string"></element>
            <element name="CHANGE_REASON" type="string"></element>
            <element name="TARGET_DATE" type="dateTime"></element>
            <element name="IMPACT" type="string"></element>
            <element name="URGENCY" type="string"></element>
            <element name="PRIORITY" type="string"></element>
            <element name="RISK_LEVEL" type="string"></element>
            <element name="STATUS" type="string"></element>
            <element name="STATUS_REASON" type="string"></element>
            <element name="MANAGER_GROUP" type="string"></element>
            <element name="AFFECTED_CUSTOMERS" type="string"></element>
            <element name="SCHEDULED_START_DATE" type="dateTime"></element>
            <element name="ACTUAL_START_DATE" type="dateTime"></element>
            <element name="SCHEDULED_END_DATE" type="dateTime"></element>
            <element name="ACTUAL_END_DATE" type="dateTime"></element>
            <element name="COMPLETED_DATE" type="dateTime"></element>
            <element name="EARLIEST_START_DATE" type="dateTime"></element>
            <element name="REQUESTED_START_DATE" type="dateTime"></element>
            <element name="REQUESTED_END_DATE" type="dateTime"></element>
            <element name="REQUESTED_SA_BEGIN_DATE" type="dateTime"></element>
            <element name="REQUESTED_SA_END_DATE" type="dateTime"></element>
            <element name="DURATION_MINUTES" type="int"></element>
            <element name="SUBMIT_DATE" type="date"></element>
            <element name="LAST_MODIFIED_DATE" type="dateTime"></element>
            <element name="IMPLEMENTOR_COMPANY" type="string"></element>
            <element name="IMPLEMENTOR_ORGANIZATION" type="string"></element>
            <element name="IMPLEMENTOR_GROUP" type="string"></element>
            <element name="CUSTOMER_COMPANY" type="string"></element>
            <element name="FAILURE_REASON" type="string"></element>
            <element name="CLOSED_DATE" type="dateTime"></element>
            <element name="REQUESTOR_COMPANY" type="string"></element>
            <element name="SITE_ID_A" type="string"></element>
            <element name="SITE_ID_B" type="string"></element>
            <element name="SITE_Name" type="string"></element>
            <element name="SERVICE_AFFECTING" type="string"></element>
            <element name="IS_CHANGE_SUCCESSFUL" type="string"></element>
            <element name="RELEASE_ID" type="string"></element>
            <element name="RELEASE_TEMPLATE" type="string"></element>
        </sequence>
    </complexType>
</schema>